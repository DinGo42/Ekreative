{
  "name": "finsweet-pure-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite --port 3000\" \"pnpx tailwindcss -i ./src/style.css -o ./dist/output.css --watch\"",
